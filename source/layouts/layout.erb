<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Sqwad Fantasy" %></title>
    <meta content="<%= current_page.data.description || 'SQWAD is a real time fantasy sports platform that allows fans to challenge pros and friends for bragging rights and brand rewards.  Sign up for our beta today!'%>" property="og:description">

    <link rel="shortcut icon" href="../images/sqwadhand.png">
    <%= stylesheet_link_tag "toolkit-startup.css" %>
    <%= stylesheet_link_tag "application-startup.css" %>
    <%= stylesheet_link_tag "landing-page.css" %>
    <%= stylesheet_link_tag "influencers-page.css" %>
    <%= stylesheet_link_tag "cascadia.css" %>
    <%= stylesheet_link_tag "new_site.css" %>
    <%= stylesheet_link_tag "schools.css" %>
    <%= stylesheet_link_tag "fc.css" %>
    <%= stylesheet_link_tag "article.css" %>
    <%= stylesheet_link_tag "jquery.mmenu.css" %>
    <%= stylesheet_link_tag "nprogress.css" %>
    <%= javascript_include_tag  "nprogress.js" %>
    <%= javascript_include_tag  "jquery.min.js" %>
    <%= javascript_include_tag  "toolkit.js" %>
    <%= javascript_include_tag  "jquery.mmenu.min.js" %>
    <%= javascript_include_tag  "application.js" %>

    <link href="https://afeld.github.io/emoji-css/emoji.css" rel="stylesheet">
    <script src="https://waitlisted.co/assets/v2/embed.js" type="text/javascript"></script>
  </head>

  <body class="<%= page_classes %>">
    <%= yield %>
    
  </body>
 
  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-72482221-1', 'auto');
    ga('send', 'pageview');

  </script>
</html>
